x <- 1:ncol(CI_pred)
lines(x,CI_pred[1,],col='blue',lwd=2)
polygon(c(x,rev(x)), c(CI_pred[2,],rev(CI_pred[3,])),
col = rgb(0,0,1,.2), border = FALSE )
}
